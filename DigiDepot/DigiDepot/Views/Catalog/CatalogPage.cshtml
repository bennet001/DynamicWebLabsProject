@model List<Product>

@{
    ViewBag.Title = "Catalog";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="jumbotron">
    <h2>This is the Catelog</h2>
</div>

<div>
    <div class="content">
        @foreach (Product c in Model)
        {
            @Html.Partial("~/Views/_CatalogPartial.cshtml", @c);
        }
    </div>
    @Html.ActionLink("Cart", "ViewCart")
</div>

